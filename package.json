{
  "name": "mp",
  "author": "Denis Narush <child.denis@gmail.com>",
  "version": "1.0.0",
  "description": "musical player",
  "homepage": "https://denisnarush.github.io/mp/",
  "bugs": {
    "url": "https://github.com/denisnarush/mp/issues"
  },
  "keywords": [
    "music",
    "soundcloud",
    "three"
  ],
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git@github.com:denisnarush/mp.git"
  },
  "license": "ISC",
  "window": {
    "toolbar": false,
    "resizable": true,
    "width": 360,
    "height": 640
  },
  "scripts": {
    "postinstall": "npm outdated",
    "install": "npm i --ignore-scripts --no-optional --no-audit",
    "test:ui:init": "backstop init",
    "test:ui:test": "backstop test",
    "test:ui:approve": "backstop approve",
    "test:js:start": "tsc -p ./tsconfig.test.json",
    "tslint": "tslint -t stylish -c tslint.json src/**/*.ts",
    "tslint:fix": "tslint -t stylish -c tslint.json src/**/*.ts --fix",
    "format": "prettier --write --config=.prettierrc.json ./",
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.es5.json"
  },
  "devDependencies": {
    "backstopjs": "^5.0.6",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "requirejs": "^2.3.6",
    "three": "^0.122.0",
    "typescript": "^4.0.5"
  }
}
